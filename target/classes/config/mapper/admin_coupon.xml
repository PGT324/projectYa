<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin_coupon">
	<!-- SQL문들을 작성하는 파일 -->
	
	<insert id="addAdmin_coupon" parameterType="vo.Admin_CouponVO">
		insert into admin_coupon values(admin_coupon_seq.nextval,#{name},#{sale},#{code},sysdate)
	</insert>
	
	
</mapper>












